    "configurations": [
        {
            "name": "Run Python Flask Server",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "\\FlaskPart\\FlaskServer.py",
                "FLASK_ENV": "development",
                "FLASK_DEBUG": "0"
            },
            "args": [
                "run",
                "--no-debugger"
            ],
            "jinja": true
        },

        {
            "preLaunchTask": {
                "task": "",
                "type": ""
            },
            "name": "Run Python GUI Application",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
            
        }
    ]
